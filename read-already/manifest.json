{

  "manifest_version": 2,
  "name": "Read-Already",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/bookmark-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["read-already.js"]
    }
  ],

   "browser_action": {
    "default_icon": "icons/bookmark-32.png",
    "default_title": "bookmark"
    
  }


}